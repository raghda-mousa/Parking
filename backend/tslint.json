{
    "extends": ["tslint:recommended", "tslint-config-prettier"],
    "rules": {
        "no-console": false,
        "no-string-literal": false,
        "no-var-requires": false,
        "interface-name": [true, "always-prefix"],
        "semicolon": [true, "always", "ignore-bound-class-methods", "ignore-interfaces"]
    }
  }